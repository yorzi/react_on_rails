<h1>Client Rendering</h1>
<%- if options.redux? %>
<%%= react_component("ReduxApp", {}, trace: true) %>
    <%- if options.server_rendering? %>
<h1>Server Rendering</h1>
<%%= react_component("ReduxServerApp", {}, prerender: true, trace: true) %>
    <%- end %>
<%- else %>
<%%= react_component("HelloWorld", trace: true) %>
  <%- if options.server_rendering? %>
<h1>Server Rendering</h1>
<%%= react_component("HelloWorld", {}, prerender: true, trace: true) %>
  <%- end %>
<%- end %>
